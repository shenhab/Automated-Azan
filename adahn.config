[Settings]
# create a google speaker group
# set the name of the group to be the value of
# speakers-group-name variable
speakers-group-name = athan

# Uncomment the needed location
# and keep the other commented.
# location = icci
location = naas

# Enable pre-Fajr Quran recitation (30 minutes before Fajr)
# Set to True to enable, False to disable
pre_fajr_enabled = True

